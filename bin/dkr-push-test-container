#!/bin/bash

# tag and push test container to ECR repo
#

set -e

SCRIPT_DIR=$( dirname "$0" )
source "$SCRIPT_DIR/dkr-config"

docker tag "${CONTAINER_NAME}:${CONTAINER_VERSION}" "${DKR_REGISTRY}/${CONTAINER_NAME}:${CONTAINER_VERSION}"
docker push "${DKR_REGISTRY}/${CONTAINER_NAME}:${CONTAINER_VERSION}"