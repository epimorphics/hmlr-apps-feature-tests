#!/bin/bash

# tag and push test container to ECR repo
#

set -e

SCRIPT_DIR=$( dirname "$0" )
source "$SCRIPT_DIR/dkr-config"

docker pull "018852084843.dkr.ecr.eu-west-1.amazonaws.com/${CONTAINER_NAME}:${CONTAINER_VERSION}"
docker tag "${DKR_REGISTRY}/${CONTAINER_NAME}:${CONTAINER_VERSION}" "${CONTAINER_NAME}:${CONTAINER_VERSION}"